		</xs:key>
		<xs:keyref name="ApplicantTypeCodeOtherKeyRef" refer="RR_SF424_1_2:ApplicantTypeCodeKey">
			<xs:selector xpath="./RR_SF424_1_2:ApplicantType/RR_SF424_1_2:ApplicantTypeCodeOtherExplanation"/>
