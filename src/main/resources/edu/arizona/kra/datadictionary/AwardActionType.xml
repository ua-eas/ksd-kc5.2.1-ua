<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="AwardActionType" parent="AwardActionType-parentBean" />
	<bean id="AwardActionType-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="edu.arizona.kra.award.actions.AwardActionType" />
		<property name="objectLabel" value="Award Action Type" />
		<property name="inquiryDefinition" >
			<ref bean="AwardActionType-inquiryDefinition" />
		</property>
		<property name="lookupDefinition" >
			<ref bean="AwardActionType-lookupDefinition" />
		</property>
		<property name="titleAttribute" value="awardActionTypeCode" />
		<property name="attributes" >
			<list>
				<ref bean="AwardActionType-awardActionTypeCode" />
				<ref bean="AwardActionType-description" />
				<ref bean="AwardActionType-triggerSubmission" />
				<ref bean="AwardActionType-triggerPrint" />
                <ref bean="AwardActionType-finalActionForBatchPrint" />
				<ref bean="AwardActionType-versionNumber" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	
	<bean id="AwardActionType-awardActionTypeCode" parent="AwardActionType-awardActionTypeCode-parentBean" />
	<bean id="AwardActionType-awardActionTypeCode-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="awardActionTypeCode" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Award Action Type Code" />
		<property name="shortLabel" value="Award Action Type Code" />
		<property name="maxLength" value="3" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
		<property name="control" >
			<bean parent="TextControlDefinition"
				p:size="3" />
		</property>
		<property name="summary" value="Award Action Type Code" />
		<property name="description" value="Award Action Type Code" />
	</bean>
	
    <bean id="AwardActionType-description" parent="AwardActionType-description-parentBean" />
    <bean id="AwardActionType-description-parentBean" abstract="true" parent="KraAttributeReferenceDummy-description" />

	<bean id="AwardActionType-triggerSubmission" parent="AwardActionType-triggerSubmission-parentBean" />
	<bean id="AwardActionType-triggerSubmission-parentBean" abstract="true" parent="AttributeDefinition">
	    <property name="name" value="triggerSubmission" />
	    <property name="forceUppercase" value="false" />
	    <property name="label" value="Trigger Submission" />
	    <property name="shortLabel" value="Trigger Submission" />
	    <property name="maxLength" value="1" />
	    <property name="control" >
	      <bean parent="CheckboxControlDefinition" />
	    </property>
	    <property name="summary" value="Trigger Submission" />
	    <property name="description" value="Trigger Submission" />
	</bean>

    <bean id="AwardActionType-triggerPrint" parent="AwardActionType-triggerPrint-parentBean" />
    <bean id="AwardActionType-triggerPrint-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="name" value="triggerPrint" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Trigger Print" />
        <property name="shortLabel" value="Trigger Print" />
        <property name="maxLength" value="1" />
        <property name="control" >
          <bean parent="CheckboxControlDefinition" />
        </property>
        <property name="summary" value="Trigger Print" />
        <property name="description" value="Trigger Print" />
    </bean>

	<bean id="AwardActionType-finalActionForBatchPrint" parent="AwardActionType-finalActionForBatchPrint-parentBean" />
	<bean id="AwardActionType-finalActionForBatchPrint-parentBean" abstract="true" parent="AttributeDefinition">
	    <property name="name" value="finalActionForBatchPrint" />
	    <property name="forceUppercase" value="false" />
	    <property name="label" value="Final Action for Batch Print" />
	    <property name="shortLabel" value="Final Action for Batch Print" />
	    <property name="maxLength" value="1" />
	    <property name="control" >
	      <bean parent="CheckboxControlDefinition" />
	    </property>
	    <property name="summary" value="Final Action for Batch Print" />
	    <property name="description" value="Final Action for Batch Print" />
	</bean>
	
	<bean id="AwardActionType-versionNumber" parent="AwardActionType-versionNumber-parentBean" />
	<bean id="AwardActionType-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
	</bean>

	<!-- Business Object Inquiry Definition -->
	<bean id="AwardActionType-inquiryDefinition" parent="AwardActionType-inquiryDefinition-parentBean" />
	<bean id="AwardActionType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
		<property name="title" value="Award Action Type Code" />
		<property name="inquirySections" >
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Award Action Type Code" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields" >
						<list>
							<bean parent="FieldDefinition"
								p:attributeName="awardActionTypeCode" 
								p:forceInquiry="true" />
							<bean parent="FieldDefinition"
								p:attributeName="description" />
						    <bean parent="FieldDefinition"
						        p:attributeName="triggerSubmission" />
						    <bean parent="FieldDefinition"
						        p:attributeName="triggerPrint" />
                            <bean parent="FieldDefinition"
                                p:attributeName="finalActionForBatchPrint" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->
	<bean id="AwardActionType-lookupDefinition" parent="AwardActionType-lookupDefinition-parentBean" />
	<bean id="AwardActionType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
		<property name="title" value="Award Action Type Lookup" />
		<property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />

		<property name="defaultSort" >
			<bean parent="SortDefinition" >
		</bean>
		</property>
		<property name="lookupFields" >
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="awardActionTypeCode" 
					p:forceInquiry="true" />
				<bean parent="FieldDefinition"
					p:attributeName="description" />
			    <bean parent="FieldDefinition"
					p:attributeName="triggerSubmission" />
				<bean parent="FieldDefinition"
					p:attributeName="triggerPrint" />
                <bean parent="FieldDefinition"
                    p:attributeName="finalActionForBatchPrint" />
                    
			</list>
		</property>
		<property name="resultFields" >
			<list>
				<bean parent="FieldDefinition"
					p:attributeName="awardActionTypeCode" 
					p:forceInquiry="true" />
				<bean parent="FieldDefinition"
					p:attributeName="description" />
			    <bean parent="FieldDefinition"
				    p:attributeName="triggerSubmission" />
				<bean parent="FieldDefinition"
					p:attributeName="triggerPrint" />
                <bean parent="FieldDefinition"
                    p:attributeName="finalActionForBatchPrint" />
			</list>
		</property>
	</bean>
</beans>
