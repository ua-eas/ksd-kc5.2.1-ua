<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="AwardPrintTempl" parent="AwardPrintTempl-parentBean" />
    <bean id="AwardPrintTempl-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="edu.arizona.kra.award.printing.AwardPrintTemplate" />
        <property name="objectLabel" value="Award Print Template" />
        <property name="inquiryDefinition" >
            <ref bean="AwardPrintTempl-inquiryDefinition" />
        </property>
        <property name="lookupDefinition" >
            <ref bean="AwardPrintTempl-lookupDefinition" />
        </property>
        <property name="titleAttribute" value="awardPrintTemplId" />
        <property name="attributes" >
            <list>
                <ref bean="AwardPrintTempl-awardPrintTemplId" />
                <ref bean="AwardPrintTempl-awardPrintTypeCode" />
                <ref bean="AwardPrintTempl-fileName" />
                <ref bean="AwardPrintTempl-printTemplate" />
                <ref bean="AwardPrintTempl-versionNumber" />
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->
    
    <bean id="AwardPrintTempl-awardPrintTemplId" parent="AwardPrintTempl-awardPrintTemplId-parentBean" />
    <bean id="AwardPrintTempl-awardPrintTemplId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="awardPrintTemplId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Award Print Templ Id" />
        <property name="shortLabel" value="Award Print Templ Id" />
        <property name="maxLength" value="12" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="12" />
        </property>
        <property name="summary" value="Award Print Templ Id" />
        <property name="description" value="Award Print Templ Id" />
    </bean>

    <bean id="AwardPrintTempl-awardPrintTypeCode" parent="AwardPrintTempl-awardPrintTypeCode-parentBean" />
    <bean id="AwardPrintTempl-awardPrintTypeCode-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="awardPrintTypeCode" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Award Print Type Code" />
        <property name="shortLabel" value="Award Print Type Code" />
        <property name="maxLength" value="3" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="3" />
        </property>
        <property name="summary" value="Award Print Type Code" />
        <property name="description" value="Award Print Type Code" />
    </bean>

    <bean id="AwardPrintTempl-fileName" parent="AwardPrintTempl-fileName-parentBean" />
    <bean id="AwardPrintTempl-fileName-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="fileName" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="File" />
        <property name="shortLabel" value="File" />
        <property name="maxLength" value="150" />
        <property name="required" value="true" />
        <property name="validationPattern" >
            <bean parent="AnyCharacterValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="150" />
        </property>
        <property name="summary" value="File" />
        <property name="description" value="File" />
    </bean>
    <bean id="AwardPrintTempl-printTemplate" parent="AwardPrintTempl-printTemplate-parentBean" />
    <bean id="AwardPrintTempl-printTemplate-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="printTemplate" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Print Template" />
        <property name="shortLabel" value="Print Template" />
        <property name="maxLength" value="4000" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="10" />
        </property>
        <property name="summary" value="Print Template" />
        <property name="description" value="Print Template" />
    </bean>

    <bean id="AwardPrintTempl-versionNumber" parent="AwardPrintTempl-versionNumber-parentBean" />
    <bean id="AwardPrintTempl-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
    </bean>

    <!-- Business Object Inquiry Definition -->
    <bean id="AwardPrintTempl-inquiryDefinition" parent="AwardPrintTempl-inquiryDefinition-parentBean" />
    <bean id="AwardPrintTempl-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Award Print Templ Id" />
        <property name="inquirySections" >
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Award Print Templ Id" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields" >
                        <list>
                            <bean parent="FieldDefinition"
                                p:attributeName="awardPrintTemplId" 
                                p:forceInquiry="true" />
                            <bean parent="FieldDefinition"
                                p:attributeName="awardPrintTypeCode" />
                            <bean parent="FieldDefinition"
                                p:attributeName="printTemplate" />
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->
    <bean id="AwardPrintTempl-lookupDefinition" parent="AwardPrintTempl-lookupDefinition-parentBean" />
    <bean id="AwardPrintTempl-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="Award Print Template Lookup" />
        <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
        <property name="defaultSort" >
            <bean parent="SortDefinition" >
        </bean>
        </property>
        <property name="lookupFields" >
            <list>
                <bean parent="FieldDefinition"
                    p:attributeName="awardPrintTemplId" 
                    p:forceInquiry="true" />
                <bean parent="FieldDefinition"
                    p:attributeName="awardPrintTypeCode" />
                <bean parent="FieldDefinition"
                    p:attributeName="printTemplate" />
            </list>
        </property>
        <property name="resultFields" >
            <list>
                <bean parent="FieldDefinition"
                    p:attributeName="awardPrintTemplId" 
                    p:forceInquiry="true" />
                <bean parent="FieldDefinition"
                    p:attributeName="awardPrintTypeCode" />
                <bean parent="FieldDefinition"
                    p:attributeName="printTemplate" />
            </list>
        </property>
    </bean>
</beans>
