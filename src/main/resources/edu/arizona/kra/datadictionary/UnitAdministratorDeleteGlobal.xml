<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
    
    <bean id="UnitAdministratorDeleteGlobal" parent="UnitAdministratorDeleteGlobal-parentBean" />
    <bean id="UnitAdministratorDeleteGlobal-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="edu.arizona.kra.global.unit.delete.UnitAdministratorDeleteGlobal" />
        <property name="objectLabel" value="Unit Administrator Global Lookup" />
        <property name="attributes">
            <list>
                <ref bean="UnitAdministratorDeleteGlobal-documentNumber" />
                <ref bean="UnitAdministratorDeleteGlobal-newCollectionRecord" />
            </list>
        </property>
    </bean>


    <!-- Attribute Definitions -->

    <bean id="UnitAdministratorDeleteGlobal-documentNumber" parent="UnitAdministratorDeleteGlobal-documentNumber-parentBean" />
    <bean id="UnitAdministratorDeleteGlobal-documentNumber-parentBean" abstract="true" parent="DocumentHeader-documentNumber" >
        <property name="control">
            <ref bean="HiddenControl" />
        </property>
    </bean>

    <bean id="UnitAdministratorDeleteGlobal-newCollectionRecord" parent="UnitAdministratorDeleteGlobal-newCollectionRecord-parentBean" />
    <bean id="UnitAdministratorDeleteGlobal-newCollectionRecord-parentBean" abstract="true" parent="GenericAttributes-newCollectionRecord" />

</beans>