<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="AwardPrintTypeTemplate" parent="AwardPrintTypeTemplate-parentBean" />
    <bean id="AwardPrintTypeTemplate-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="edu.arizona.kra.award.printing.AwardPrintTypeTemplate" />
		<property name="objectLabel" value="Award Print Type" />
        <property name="inquiryDefinition" >
            <ref bean="AwardPrintType-inquiryDefinition" />
        </property>
        <property name="lookupDefinition" >
            <ref bean="AwardPrintTypeTemplate-lookupDefinition" />
        </property>
        <property name="titleAttribute" value="awardPrintTypeCode" />
        <property name="attributes" >
            <list>
                <ref bean="AwardPrintTypeTemplate-awardPrintTypeCode" />
                <ref bean="AwardPrintTypeTemplate-description" />
                <ref bean="AwardPrintTypeTemplate-moduleId" />
                <ref bean="AwardPrintTypeTemplate-moduleDescription" />
                <ref bean="AwardPrintTypeTemplate-versionNumber" />
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->
    
    <bean id="AwardPrintTypeTemplate-awardPrintTypeCode" parent="AwardPrintTypeTemplate-awardPrintTypeCode-parentBean" />
    <bean id="AwardPrintTypeTemplate-awardPrintTypeCode-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="awardPrintTypeCode" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Award Print Type Code" />
        <property name="shortLabel" value="Award Print Type Code" />
        <property name="maxLength" value="3" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition"
                p:size="3" />
        </property>
        <property name="summary" value="Award Print Type Code" />
        <property name="description" value="Award Print Type Code" />
    </bean>

    <bean id="AwardPrintTypeTemplate-description" parent="AwardPrintTypeTemplate-description-parentBean" />
    <bean id="AwardPrintTypeTemplate-description-parentBean" abstract="true" parent="KraAttributeReferenceDummy-description" />

    <bean id="AwardPrintTypeTemplate-moduleId" parent="AwardPrintTypeTemplate-moduleId-parentBean" />
    <bean id="AwardPrintTypeTemplate-moduleId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="moduleId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Module Id" />
        <property name="shortLabel" value="Module Id" />
        <property name="maxLength" value="1" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="required" value="true" />
        <property name="control" >
            <bean parent="SelectControlDefinition"
                  p:valuesFinderClass="edu.arizona.kra.award.printing.AwardPrintTypeModuleIdValuesFinder"
                  p:includeKeyInLabel="false" />
        </property>
        <property name="summary" value="Module Id" />
        <property name="description" value="Module Id" />
    </bean>

    <bean id="AwardPrintTypeTemplate-moduleDescription" parent="AwardPrintTypeTemplate-moduleDescription-parentBean" />
    <bean id="AwardPrintTypeTemplate-moduleDescription-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="module.description" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Module" />
        <property name="shortLabel" value="Module" />
        <property name="control">
            <bean parent="TextControlDefinition"/>
        </property>
        <property name="summary" value="Module" />
        <property name="description" value="Module" />
    </bean>

    <bean id="AwardPrintTypeTemplate-versionNumber" parent="AwardPrintTypeTemplate-versionNumber-parentBean" />
    <bean id="AwardPrintTypeTemplate-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
    </bean>

    <!-- Business Object Inquiry Definition -->
    <bean id="AwardPrintTypeTemplate-inquiryDefinition" parent="AwardPrintTypeTemplate-inquiryDefinition-parentBean" />
    <bean id="AwardPrintTypeTemplate-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Award Print Type Code" />
        <property name="inquirySections" >
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Award Print Type Code" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields" >
                        <list>
                            <bean parent="FieldDefinition"
                                p:attributeName="awardPrintTypeCode" 
                                p:forceInquiry="true" />
                            <bean parent="FieldDefinition"
                                p:attributeName="description" />
                            <bean parent="FieldDefinition"
                                p:attributeName="moduleId" />
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->
    <bean id="AwardPrintTypeTemplate-lookupDefinition" parent="AwardPrintTypeTemplate-lookupDefinition-parentBean" />
    <bean id="AwardPrintTypeTemplate-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="Award Print Type Lookup" />
        <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />

        <property name="defaultSort" >
            <bean parent="SortDefinition" >
        </bean>
        </property>
        <property name="lookupFields" >
            <list>
                <bean parent="FieldDefinition"
                    p:attributeName="awardPrintTypeCode" 
                    p:forceInquiry="true" />
                <bean parent="FieldDefinition"
                    p:attributeName="description" />
                <bean parent="FieldDefinition"
                    p:attributeName="moduleId" />
            </list>
        </property>
        <property name="resultFields" >
            <list>
                <bean parent="FieldDefinition"
                    p:attributeName="awardPrintTypeCode" 
                    p:forceInquiry="true" />
                <bean parent="FieldDefinition"
                    p:attributeName="description" />
                <bean parent="FieldDefinition"
                    p:attributeName="module.description" />
            </list>
        </property>
    </bean>
</beans>
